<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zte.medicine.entity.SaleComment" table="t_sale_comment" schema="medicine">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="native"></generator>
        </id>
        <property name="medicineCode">
            <column name="MedicineCode" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="price">
            <column name="Price" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="number">
            <column name="Number" sql-type="int(64)" not-null="true"/>
        </property>
        <property name="amount">
            <column name="Amount" sql-type="double(64)" precision="64"/>
        </property>
        <many-to-one name="saleBySaleNum" class="com.zte.medicine.entity.Sale">
            <column name="SaleNum"/>
        </many-to-one>
    </class>
</hibernate-mapping>