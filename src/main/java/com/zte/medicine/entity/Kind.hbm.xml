<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zte.medicine.entity.Kind" table="t_kind" schema="medicine">
        <id name="kindCode">
            <column name="KindCode" sql-type="varchar(20)" length="20"/>
        </id>
        <property name="kindRemark">
            <column name="KindRemark" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>