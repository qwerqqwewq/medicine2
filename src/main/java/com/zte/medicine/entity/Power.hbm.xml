<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zte.medicine.entity.Power" table="t_power" schema="medicine">
        <id name="id">
            <column name="id" sql-type="int(20)"/>
        </id>
        <property name="power">
            <column name="power" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>